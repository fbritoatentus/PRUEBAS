#include <iostream>
#include <string>
using namespace std;

template <typenameT>
incline T const& Max (T const& a, T const& b)
{
	return a<b? b:a;
}

int main() {
	int i = 39;
	int j = 20;
	
	cout <<"Max(i,j): " << Max(i,j)<< endl;
	
	double f1 = 13.5;
	double f2 = 20.7;
	
	cout <<"Max(i,j): "<< Max(i,j)<< endl;
	
	string f1 = "!!Quee";
	string f2 = "Taal¡¡";
	
	cout <<"Max(i,j): "<< Max(i,j)<< endl;
	
	return 0;
}	
	
}
